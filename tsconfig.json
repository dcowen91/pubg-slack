{
"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"strictNullChecks": true,
	"removeComments": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	 "inlineSources": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": [
				"node_modules/*",
				"src/types/*"
			]
		}
	},
	"include": [
		"src/**/*",
		"typings/**/*"
	]
}